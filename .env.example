# OpenClaw Avatar - Environment Variables
# Copy this file to .env and fill in your values

# Required - Get from https://simli.com
SIMLI_API_KEY=

# Required - Get from https://elevenlabs.io
ELEVENLABS_API_KEY=

# Optional - Override default voice ID from config
# ELEVENLABS_VOICE_ID=

# Optional - For Slack integration
# SLACK_BOT_TOKEN=xoxb-...

# Optional - For OpenClaw gateway authentication
# OPENCLAW_TOKEN=
